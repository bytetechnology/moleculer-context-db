{
  "name": "moleculer-context-db",
  "version": "0.0.11",
  "description": "A database tool for providing the database session as part of the context",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc -p src/tsconfig.json",
    "lint": "eslint \"{src,{tests,mocks}}/**/*.{js,ts}\"",
    "format": "prettier --write \"{src,{tests,mocks}}/**/*.{js,ts}\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "test": "jest --coverage",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bytetechnology/moleculer-context-db.git"
  },
  "keywords": [
    "moleculer",
    "microservices",
    "database",
    "session",
    "transaction"
  ],
  "author": "David Sornberger <davids@bytetechnology.co>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bytetechnology/moleculer-context-db/issues"
  },
  "homepage": "https://github.com/bytetechnology/moleculer-context-db#readme",
  "dependencies": {
    "mikro-orm": "^3.4.1",
    "moleculer": "^0.14.4",
    "uuid": "7.0.2"
  },
  "devDependencies": {
    "@types/jest": "25.1.4",
    "@types/mongodb": "3.5.2",
    "@types/node": "13.9.1",
    "@types/uuid": "7.0.0",
    "@typescript-eslint/eslint-plugin": "2.23.0",
    "@typescript-eslint/parser": "2.23.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-prettier": "3.1.2",
    "jest": "25.1.0",
    "moleculer-decorators": "1.2.0",
    "prettier": "1.19.1",
    "run-rs": "^0.6.2",
    "sqlite3": "4.1.1",
    "ts-jest": "25.2.1",
    "ts-node": "8.6.2",
    "typescript": "3.8.3"
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": ".",
    "coverageDirectory": "tests/coverage",
    "roots": [
      "tests"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".*\\.(test|spec).(ts|js)$",
    "globals": {
      "ts-jest": {
        "tsConfig": "./tsconfig.json"
      }
    },
    "setupFiles": [],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
