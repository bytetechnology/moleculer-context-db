{
  "name": "moleculer-context-db",
  "version": "0.0.5",
  "description": "A database tool for providing the database session as part of the context",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": ["lib"],
  "scripts": {
    "prepublish": "npm run lint && npm run clean && npm run build",
    "clean": "rm -rf coverage/* lib/*",
    "test": "npm run build && jest --coverage && rm test_sqlite_db",
    "lint": "eslint src/**/*.ts",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bytetechnology/moleculer-context-db.git"
  },
  "keywords": [
    "moleculer",
    "microservices",
    "database",
    "session",
    "transaction"
  ],
  "author": "David Sornberger <davids@bytetechnology.co>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bytetechnology/moleculer-context-db/issues"
  },
  "homepage": "https://github.com/bytetechnology/moleculer-context-db#readme",
  "jest": {
    "coverageDirectory": "<rootDir>/coverage",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js"
    ],
    "testMatch": [
      "<rootDir>/lib/**/*.spec.js"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    }
  },
  "dependencies": {
    "mikro-orm": "3.3.5",
    "moleculer": "0.14.3"
  },
  "devDependencies": {
    "@types/jest": "25.1.3",
    "@types/node": "13.7.7",
    "@types/uuid": "7.0.0",
    "@typescript-eslint/eslint-plugin": "2.22.0",
    "@typescript-eslint/parser": "2.22.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-prettier": "3.1.2",
    "jest": "25.1.0",
    "moleculer-decorators": "1.2.0",
    "prettier": "1.19.1",
    "sqlite3": "4.1.1",
    "ts-jest": "25.2.1",
    "ts-node": "8.6.2",
    "typescript": "3.8.3",
    "uuid": "7.0.2"
  }
}
